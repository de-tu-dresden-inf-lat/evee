<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <parent>
        <groupId>de.tu-dresden.inf.lat.evee</groupId>
        <artifactId>evee-protege</artifactId>
        <version>0.1-SNAPSHOT</version>
    </parent>
    <modelVersion>4.0.0</modelVersion>

    <artifactId>evee-protege-elk-proof-extractor</artifactId>
    <packaging>bundle</packaging>

    <properties>
        <maven.compiler.source>8</maven.compiler.source>
        <maven.compiler.target>8</maven.compiler.target>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
    </properties>

    <repositories>
        <repository>
            <id>EveeLocalMavenRepository</id>
            <name>EveeLocalMavenRepository</name>
            <url>file:${project.basedir}/../../lib</url>
        </repository>
    </repositories>

    <dependencies>
        <dependency>
            <groupId>de.tu-dresden.inf.lat.evee</groupId>
            <artifactId>evee-protege-core</artifactId>
            <version>0.1-SNAPSHOT</version>
        </dependency>
        <dependency>
            <groupId>de.tu-dresden.inf.lat.evee</groupId>
            <artifactId>evee-elk-proof-extractor-owlapi4</artifactId>
            <version>0.1-SNAPSHOT</version>
        </dependency>
    </dependencies>


    <build>
        <resources>
            <resource>
                <directory>src/main/resources</directory>
                <includes>
                    <include>plugin.xml</include>
                </includes>
                <filtering>true</filtering>
            </resource>
        </resources>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>3.0</version>
                <configuration>
                    <source>8</source>
                    <target>8</target>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.apache.felix</groupId>
                <artifactId>maven-bundle-plugin</artifactId>
                <version>3.5.0</version>
                <extensions>true</extensions>
                <configuration>
                    <instructions>
                        <Bundle-SymbolicName>${project.artifactId};singleton:=true</Bundle-SymbolicName>
                        <Import-Package>
                            sun.misc.*;resolution:=optional,
                            <!--                            org.apache.batik.transcoder.*;resolution:=optional,-->
                            <!--                            org.apache.tools.ant.*;resolution:=optional,-->
                            <!--                            org.graalvm.polyglot.*;resolution:=optional,-->
                            de.tu_dresden.inf.lat.evee.proofs.*,
                            de.tu_dresden.inf.lat.evee.protege.abstractProofService.*,
                            com.ibm.uvm.tools.*;resolution:=optional,
                            com.sun.jdmk.comm.*;resolution:=optional,
                            javax.jmdns.*;resolution:=optional,
                            javax.jms.*;resolution:=optional,
                            org.apache.geronimo.osgi.registry.api.*;resolution:=optional,
                            org.osgi.service.log.*;resolution:=optional,
                            *
                        </Import-Package>
                        <Private-Package>
                            de.tu_dresden.inf.lat.evee.proofGenerators.*,
                            de.tu_dresden.inf.lat.evee.data.*,
                            de.tu_dresden.inf.lat.evee.inOut.*,
                            de.tu_dresden.inf.lat.evee.tools.*,
                            <!--                            de.tu_dresden.inf.lat.forgettingBasedProofs.*;-split-package:=merge-last,-->
                            org.apache.log4j.*;-split-package:=merge-last,
                            javax.mail.*,
                            javax.activation.*,
                            org.apache.geronimo.mail.*,
                            org.apache.geronimo.osgi.locator.*;-split-package:=merge-last,
                            org.liveontologies.owlapi.proof.*,
                            org.semanticweb.elk.*;-split-package:=merge-last,
                            <!--                            org.semanticweb.elk.owlapi.*,-->
                            <!--                            org.semanticweb.elk.exceptions.*,-->
                            <!--                            org.semanticweb.elk.loading.*,-->
                            <!--                            org.semanticweb.elk.owl.iris.*,-->
                            <!--                            org.semanticweb.elk.owl.inferences.*,-->
                            <!--                            org.semanticweb.elk.matching.*,-->
                            <!--                            org.semanticweb.elk.owl.implementation.*,-->
                            <!--                            org.semanticweb.elk.owl.interfaces.*,-->
                            <!--                            org.semanticweb.elk.owl.managers.*,-->
                            <!--                            org.semanticweb.elk.owl.parsing.*,-->
                            <!--                            org.semanticweb.elk.owl.predefined.*,-->
                            <!--                            org.semanticweb.elk.owl.comparison.*,-->
                            <!--                            org.semanticweb.elk.owl.printers.*,-->
                            <!--                            org.osgi.service.log.*,-->
                            <!--                            org.apache.commons.lang3.*,-->
                            <!--                            org.apache.commons.exec.*,-->
                            <!--                            org.apache.env.*,-->
                            <!--                            forgetting.*,-->
                            <!--                            checkfrequency.*,-->
                            <!--                            checkreducedform.*,-->
                            <!--                            checkexistence.*,-->
                            <!--                            concepts.*,-->
                            <!--                            connectives.*,-->
                            <!--                            converter.*,-->
                            <!--                            extractor.*,-->
                            <!--                            formula.*,-->
                            <!--                            individual.*,-->
                            <!--                            inferencing.*,-->
                            <!--                            preprocessing.*,-->
                            <!--                            roles.*,-->
                            <!--                            guru.nidi.graphviz.attribute.*,-->
                            <!--                            guru.nidi.graphviz.engine.*,-->
                            <!--                            guru.nidi.graphviz.model.*,-->
                            <!--                            guru.nidi.graphviz.service.*,-->
                            <!--                            com.eclipsesource.v8.*,-->
                            <!--                            com.kitfox.svg.*,-->
                            com.google.common.collect.*;-split-package:=merge-last,
                            com.google.common.base.*;-split-package:=merge-last,
                            com.google.common.math.*;-split-package:=merge-last,
                            com.google.common.primitives.*;-split-package:=merge-last,
                        </Private-Package>
                    </instructions>
                </configuration>
            </plugin>
        </plugins>
    </build>

</project>